      "required": "true"
    },
    "court_type": {
      "type": "string",
      "title": "Court type",
      "enum": [
        "Supreme Court",
        " High Court",
        " Civil Judge",
        " Metropolitan/Judicial Magistrate",
        " District and Sessions Judge",
        "Small Causes Court"
